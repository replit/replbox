{
  "name": "@replit/replbox",
  "version": "1.0.3",
  "description": "A sandboxed browser REPL",
  "main": "dist/client.bundle.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "prepublishOnly": "yarn build && yarn test",
    "clean": "rm -rf ./dist",
